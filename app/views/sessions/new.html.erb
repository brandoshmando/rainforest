<h1>Sign in!</h1>
<p><%= link_to "//Home", root_path %></p>
<hr>
<%= form_tag sessions_path %>
<div class="field">
  <%= label_tag :email %><br/>
  <%= text_field_tag :email %>
</div>

<div class="field">
  <%= label_tag :password %></br>
  <%= password_field_tag :password %>
</div>

<div class="actions">
  <%= submit_tag "Log in" %>
</div>
